{
    "name": "manuelceron/laralight",
    "description": "Un paquete Laravel para modelos dinámicos y generación de APIs ligeras.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Manuelceron\\Laralight\\": "src/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "filament/filament": "^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Manuelceron\\Laralight\\LaralighServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --provider=\"Manuelceron\\Laralight\\LaralightServiceProvider\""
        ]
    },
    "authors": [
        {
            "name": "Manuel Cerón",
            "email": "manuelceron1@gmail.com",
            "homepage": "https://www.sam.net.co"
        }
    ],
    "license": "MIT"
}
